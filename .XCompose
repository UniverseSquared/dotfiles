include "%L"

# https://www.unicode.org/Public/UNIDATA/UnicodeData.txt

# maths
<Multi_key> <plus> <minus> : "±" # PLUS-OR-MINUS SIGN
<Multi_key> <minus> <plus> : "∓" # MINUS-OR-PLUS SIGN

# greek letters
<Multi_key> <G> <a> : "α" # GREEK SMALL LETTER ALPHA
<Multi_key> <G> <p> : "π" # GREEK SMALL LETTER PI

# logic
<Multi_key> <L> <t> : "⊤" # DOWN TACK
<Multi_key> <L> <f> : "⊥" # UP TACK
<Multi_key> <L> <n> : "¬" # NOT SIGN
<Multi_key> <L> <a> : "∧" # LOGICAL AND
<Multi_key> <L> <o> : "∨" # LOGICAL OR
